                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="script">import org.apache.jmeter.services.FileServer;

// Declare File Name
String fileName = &quot;\\flight_details.txt&quot;;
String filePath = FileServer.getFileServer().getBaseDir() + fileName;

// Create file if it doesn&apos;t exist
File file = new File(filePath);
try {
	boolean fileExists = file.exists();
    FileWriter fileWriter = new FileWriter(filePath, true);
    if (!fileExists) {
        // Write title if file is newly created
        fileWriter.write(&quot;Flight Details\n&quot;);
    }
    // Write paid flight details
    fileWriter.write(&quot;Id: &quot; + vars.get(&quot;c_paidFlightDetails_g1&quot;) + &quot;\t&quot;);
    fileWriter.write(&quot;Amount: &quot; + vars.get(&quot;c_paidFlightDetails_g2&quot;) + &quot; USD\n&quot;);
    fileWriter.close();
} catch (Exception e) {
    log.error(&quot;Failed to write to file: &quot; + e.getMessage());
}
</stringProp>
