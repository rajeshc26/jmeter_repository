              <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Capture all depature and destination cities" enabled="true">
                <boolProp name="resetInterpreter">false</boolProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="script">import org.apache.jmeter.services.FileServer;

// Declare File Name
String fileName = &quot;\\departure_destination.txt&quot;;
String filePath = FileServer.getFileServer().getBaseDir() + fileName;

// Write the extracted values to the file
FileOutputStream outputStream = new FileOutputStream(filePath);
OutputStreamWriter outputStreamWriter = new OutputStreamWriter(outputStream, &quot;UTF-8&quot;);

// Get all depature cities
String departure = &quot;c_toPortValues_&quot;;
int i = 1;
outputStreamWriter.write(&quot;Departure Cities:\n&quot;);
while(true) {
	String value = vars.get(departure+i);
	if (value == null) {
        break; 
    }
    outputStreamWriter.write(value + &quot;\n&quot;);
    i++;
}

// Get all destination cities
String destinatoin = &quot;c_fromPortValues_&quot;;
int i = 1;
outputStreamWriter.write(&quot;\n\nDestination Cities: \n&quot;);
while(true) {
	String value = vars.get(destinatoin+i);
	if (value == null) {
        break; 
    }
    outputStreamWriter.write(value + &quot;\n&quot;);
    i++;
}

outputStreamWriter.close();
</stringProp>
